<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#406D6D6D" />
            <gradient android:angle="180" android:startColor="#1faddf" android:endColor="#20baf4" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#406D6D6D" />
            <gradient android:angle="180" android:startColor="#1faddf" android:endColor="#20baf4" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="3dip" android:color="#5C989898" />
            <gradient android:angle="0" android:startColor="#FFFFFF" android:endColor="#FFFFFF" />
        </shape>
    </item>
</selector>